#!/home/matek/projects/nuvolos/tools/assignment-collect/nuvolos_collect/venv/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'nuvolos-collect','console_scripts','nvcollect'
__requires__ = 'nuvolos-collect'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('nuvolos-collect', 'console_scripts', 'nvcollect')()
    )
